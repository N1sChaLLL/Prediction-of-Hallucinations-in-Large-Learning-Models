        "#A3\n",
        "df1 = df_features.iloc[0]\n",
        "df2 = df_features.iloc[1]\n",
        "r = 1\n",
        "dist = 0\n",
        "newl = []\n",
        "newl2 = []\n",
        "for j in range (10):\n",
        "  for i in range(len(df1)):\n",
        "    dist += abs(((df1.iloc[i] - df2.iloc[i])**r))\n",
        "  dist = dist**(1/r)\n",
        "  newl2.append(r)\n",
        "  r+=1\n",
        "  newl.append(dist)\n",
        "sns.lineplot(y=newl,x=newl2)\n",
        "plt.ylabel('Distance')\n",
        "plt.xlabel('r')\n",
        "plt.title('Scatter Plot of Distance vs r')\n",
        "plt.xlim(1, 10)\n",
        "plt.ylim(1,10)\n",
